page {
  width: 100%;
  display: flex;
  flex-direction: column;
  height: 100%;
  align-items: center;
}

.container{
  width: 100%;
  height: 40%;
  display: flex;
  align-items: center;
  justify-content: center;
}

.container__iconContainer{
  height: 300rpx;
  width: 300rpx;
}

.container__iconContainer image{
  height: 100%;
  width: 100%;
  border-radius: 50%;
}

.confirm{
  color: white;
  background-color: #9cc3f9;
  width: 90%;
  margin-top: 50rpx;
}